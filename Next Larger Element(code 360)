class Solution:
    def nextLargerElement(self, arr):
        stack = []
        ans = []
        n = len(arr)
        for ind in range(n-1, -1, -1) :
            curelement = arr[ind]
            while len(stack)!=0 and stack[-1]<=curelement :
                stack.pop()
            if len(stack) == 0:
                ans.append(-1)
            else :
                ans.append(stack[-1])
            stack.append(curelement)
        return ans[::-1]
